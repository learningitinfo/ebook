<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--namespace的值必须与对应mapper接口的全名一致-->
<mapper namespace="com.leshan.ebook.mapper.OrderItemMapper">
    <!--批量插入数据-->
   <insert id="addItem">
       insert into mall_order_item values
       <foreach collection="list" item="item" separator=",">
           (default,#{item.orderid},#{item.goodsid},#{item.nums},#{item.price})
       </foreach>
   </insert>
</mapper>